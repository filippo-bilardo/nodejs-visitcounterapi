{
  "name": "visit-counter-api",
  "version": "1.0.0",
  "description": "API per contatore di visite multi-sito con embed JavaScript",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -rf node_modules package-lock.json",
    "docker:build": "docker build -t visit-counter .",
    "docker:run": "docker run -p 3000:3000 visit-counter"
  },
  "keywords": [
    "nodejs",
    "express",
    "counter",
    "analytics",
    "embed",
    "cors",
    "api"
  ],
  "author": "Visit Counter Service",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "redis": "^4.6.7",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "eslint": "^8.45.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/visit-counter-api"
  },
  "bugs": {
    "url": "https://github.com/your-org/visit-counter-api/issues"
  }
}
